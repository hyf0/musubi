{
  "$schema": "https://oxc.rs/schema/oxlintrc.json",
  "rules": {
    "typescript": {
      "no-explicit-any": "warn",
      "no-unused-vars": "warn"
    },
    "import": {
      "no-cycle": "warn"
    },
    "unicorn": {
      "no-null": "off"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  }
}
